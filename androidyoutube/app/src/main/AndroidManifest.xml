<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.android.androidyoutube">

    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity
            android:name=".Listss"
            android:theme="@android:style/Theme.Light">
        </activity>
        <activity android:name=".TextPlay">
        </activity>
        <activity android:name=".RecViewAnaClock">
        </activity>
        <activity android:name=".Email">
        </activity>
        <activity
            android:name=".Photo"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Send">
        </activity>
        <activity android:name=".Get">
        </activity>
        <activity
            android:name=".About"
            android:theme="@android:style/Theme.Dialog">
        </activity>
        <activity android:name=".Prefs">
        </activity>
        <activity android:name=".Beginner">
        </activity>
        <activity
            android:name=".GFX"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".GFXSurface"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".SoundStuff">
        </activity>
        <activity android:name=".Slider">
        </activity>
        <activity android:name=".Tabs">
        </activity>
        <activity android:name=".SimpleBrowser">
        </activity>
        <activity android:name=".Flipper">
        </activity>
        <activity android:name=".SharedPrefs">
        </activity>
        <activity android:name=".InternalData">
        </activity>
        <activity android:name=".ExternalData">
        </activity>
        <activity android:name=".SQlite1">
        </activity>
        <activity android:name=".SQLView">
        </activity>
        <activity
            android:name=".Accelerate"
            android:screenOrientation="portrait">
        </activity>

        <receiver
            android:name=".PointlessWidget"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>

            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/widget_stuff"/>
        </receiver>

        <activity android:name=".WidgetConfig">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_CONFIGURE"/>
            </intent-filter>
        </activity>
        <activity android:name=".GLExample">
        </activity>
        <activity android:name=".GLCubeEx">
        </activity>
        <activity android:name=".Voice">
        </activity>
        <activity android:name=".TextVoice">
        </activity>
        <activity android:name=".SeekBarVolume">
        </activity>
    </application>

</manifest>