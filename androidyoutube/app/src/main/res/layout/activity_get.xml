<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.androidyoutube.Get">

    <EditText
        android:id="@+id/etSend"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/bSA"
        android:text="StartActivity"
        android:layout_below="@+id/etSend"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/bSAFR"
        android:text="Start Activity For Result"
        android:layout_toLeftOf="@+id/bSA"
        android:layout_alignTop="@+id/bSA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/tvGot"
        android:text="Text"
        android:layout_below="@+id/bSAFR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</RelativeLayout>
